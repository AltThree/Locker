{
    "name": "alt-three/locker",
    "description": "A lock manager for Laravel 5",
    "keywords": ["locker", "locking", "Locker", "Alt Three"],
    "license": "MIT",
    "authors": [
        {
            "name": "Alt Three",
            "email": "support@alt-three.com"
        }
    ],
    "require": {
        "php": "^7.1.3",
        "illuminate/contracts": "^5.5|^6.0",
        "illuminate/support": "^5.5|^6.0",
        "illuminate/redis": "^5.5|^6.0",
        "predis/predis": "^1.1"
    },
    "require-dev": {
        "graham-campbell/analyzer": "^2.1",
        "graham-campbell/testbench": "^5.2",
        "phpunit/phpunit": "^6.5|^7.0|^8.0"
    },
    "autoload": {
        "psr-4": {
            "AltThree\\Locker\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "AltThree\\Tests\\Locker\\": "tests/"
        }
    },
    "config": {
        "preferred-install": "dist"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "5.1-dev"
        },
        "laravel": {
            "providers": [
                "AltThree\\Locker\\LockerServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
